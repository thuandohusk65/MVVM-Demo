<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.youtubemvvm.home.view.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_savedVideoFragment"
            app:destination="@id/savedVideoFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedVideoFragment"
        android:name="com.example.youtubemvvm.savedvideo.view.SavedVideoFragment"
        android:label="fragment_saved_video"
        tools:layout="@layout/fragment_saved_video" />
</navigation>